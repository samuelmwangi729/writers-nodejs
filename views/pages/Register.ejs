
<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->

<%- include('head.ejs') %>
<body data-spy="scroll" data-target=".navbar" data-offset="60">
    <div class="se-pre-con"></div>
    <%- include('navbar.ejs')%>
    <!--page banner start-->
    <div class="page-banner-brd">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="inner-brd-text text-center">
                        <h1><%= title %></h1>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb justify-content-center">
                                <li class="breadcrumb-item"><a href="/">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page"><%= title %></li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div><!--/.container-->
        <span class="banner-frame-one"></span>
        <span class="banner-frame-two"></span>
        <span class="banner-frame-three"></span>
        <span class="banner-frame-four"></span>
        <div class="shape-top-left" data-aos="fade-right" data-aos-delay="500"></div>
        <div class="shape-bottom-right" data-aos="fade-left" data-aos-anchor-placement="top-bottom" data-aos-delay="1500"></div>
    </div>
    <!--page banner end-->
    <div class="container text-center">
        <div class="row">
            <div class="col-sm-4 offset-sm-4" style="padding: 20px;">
                <h2 class="text-center">
                    Register Your Account
                </h2>
                <form action="#" class="mb-5">
                    <fieldset >
                        <div class="input-gruoup">
                            <label for="Username" class="label-control">Username</label>
                            <input type="text" name="Username" placeholder="Enter your Username Here" id="" class="form-control" 
                            style="background-color: white !important;border: 1px solid black; border-radius: 0px !important">
                        </div>
                        <div class="input-gruoup">
                            <label for="Username" class="label-control">Password</label>
                            <input type="password" name="Password" placeholder="Enter your Password Here" id="" class="form-control" 
                            style="background-color: white !important;border: 1px solid black; border-radius: 0px !important">
                        </div>
                        <div class="input-gruoup">
                            <label for="Username" class="label-control">Confirm Password</label>
                            <input type="password" name="Password" placeholder="Enter your Password Confirmation Here" id="" class="form-control" 
                            style="background-color: white !important;border: 1px solid black; border-radius: 0px !important">
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <button class="btn btn-primary btn-block text-center mt-2" style="width: 100%;">Create Account</button>
                            </div>
                        </div>
                    </fieldset>
                    <div class="row">
                        <div class="col-sm-6">
                            <span>
                                <a href="/Reset">Reset Your Password? </a>
                            </span>
                        </div>
                        <div class="col-sm-6">
                            <span>
                                <a href="/Login">
                                    Login Your Account
                                </a>
                            </span>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!--footer area start-->
    <%- include('footer.ejs') %>
</body>
</html>